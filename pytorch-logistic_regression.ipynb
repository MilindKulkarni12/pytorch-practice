      "epoch: 90 - loss: 0.268\n",
      "epoch: 100 - loss: 0.255\n"
     ]
    }
   ],
   "source": [
    "# Train\n",
    "iterations = 100\n",
    "for epoch in range(1, iterations+1):\n",
    "    # forward pass\n",
    "    y_pred = model(X_train,)\n",
    "\n",
    "    # calculate loss\n",
    "    loss = criterion(y_pred, y_train)\n",
    "\n",
    "    # backward pass\n",
    "    loss.backward()\n",
    "\n",
    "    # update weights\n",
    "    optimizer.step()\n",
    "\n",
    "    # reset weights\n",
    "    optimizer.zero_grad()\n",
    "\n",
    "    if epoch % 10 == 0:\n",
    "        print(f\"epoch: {epoch} - loss: {loss.item():.3f}\")"
   ],
   "metadata": {
    "collapsed": false
   }
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "tensor(0.9035)\n"
     ]
    }
   ],
   "source": [
    "with torch.no_grad():\n",
    "    y_pred = model(X_test)\n",
    "    y_pred_class = y_pred.round()\n",
    "    accuracy = y_pred_class.eq(y_test).sum() / float(y_test.shape[0])\n",
    "    print(f"Accuracy: {accuracy}")"
   ],
